{
    "package": {
        "name": "datarator", // Bintray package name
        "repo": "datarator", // Bintray repository name
        "subject": "datarator", // Bintray subject (user or organization)
        "website_url": "github.com/datarator/datarator",
        "issue_tracker_url": "http://github.com/datarator/datarator/issues",
        "vcs_url": "http://github.com/datarator/datarator.git",
        "github_use_tag_release_notes": true,
        "licenses": [
            "MIT"
        ],
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "0.1.0",
        "vcs_tag": "0.1.0",
        "gpgSign": false
    },
    "files": [
        {
            "includePattern": "./dist/datarator_([a-z].*[64m])", /* [a-z] => not linux packages (they hold version number at place) && [62m] => non-windows binaries (386/64/arm)  */
            "uploadPattern": "0.1.0/datarator_$1/datarator"
        },
        {
            "includePattern": "./dist/datarator_([a-z].*).exe", /* windows binaries */
            "uploadPattern": "0.1.0/datarator_$1/datarator.exe"
        }
    ],
    "publish": true
}